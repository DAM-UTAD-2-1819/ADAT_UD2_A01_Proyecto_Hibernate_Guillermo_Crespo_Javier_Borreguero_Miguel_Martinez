<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="Modelo.personajes" table="personajes">
		<id name="ID" type="int">
			<column name="ID" />
			<generator class="increment" />
		</id>

		<property name="Nombre_Personaje" type="string">
			<column name="Nombre_Personaje" length="30" not-null="true" />
		</property>
		
		<many-to-one name="videojuegos" column="ID" not-null="true"/>
	</class>

</hibernate-mapping>